package org.papervision3d.core.render.command {	import org.papervision3d.materials.special.VectorShapeMaterial;		import org.papervision3d.materials.special.LineMaterial;		//import org.papervision3d.core.layers.RenderLayer;		import org.papervision3d.objects.special.VectorShape3D;		import org.papervision3d.core.render.data.RenderSessionData;		import org.papervision3d.core.render.command.RenderableListItem;	import org.papervision3d.core.render.command.IRenderListItem;		/**	 * @author Mark Barcinski	 */	public class RenderVectorShape extends RenderableListItem implements IRenderListItem {		public var vectorShape : VectorShape3D;		public var renderer : VectorShapeMaterial;				public function RenderVectorShape(vectorShape : VectorShape3D) {			this.vectorShape = vectorShape;						super();		}				public function uninit():void {		    this.vectorShape = null;		    this.renderer = null;		}				public override function render(renderSessionData : RenderSessionData) : void {								renderer.drawShape(vectorShape, renderSessionData.container.graphics, renderSessionData);		}	}}